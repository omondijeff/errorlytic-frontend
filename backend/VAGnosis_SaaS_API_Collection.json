        "type": "text/javascript",
        "exec": [
          "// Auto-refresh token if needed",
          "const token = pm.collectionVariables.get('accessToken');",
          "if (!token) {",
          "    console.log('No access token found. Please login first.');",
          "}"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Global test script",
          "if (pm.response.code >= 400) {",
          "    console.log('Error Response:', pm.response.text());",
          "}",
          "",
          "// Auto-save IDs from responses",
          "if (pm.response.json && pm.response.json().data) {",
          "    const data = pm.response.json().data;",
          "    if (data.upload && data.upload._id) {",
          "        pm.collectionVariables.set('uploadId', data.upload._id);",
          "    }",
          "    if (data.analysis && data.analysis._id) {",
          "        pm.collectionVariables.set('analysisId', data.analysis._id);",
          "    }",
          "    if (data.walkthrough && data.walkthrough._id) {",
          "        pm.collectionVariables.set('walkthroughId', data.walkthrough._id);",
          "    }",
          "    if (data.quotation && data.quotation._id) {",
          "        pm.collectionVariables.set('quotationId', data.quotation._id);",
          "    }",
          "    if (data.organization && data.organization._id) {",
          "        pm.collectionVariables.set('orgId', data.organization._id);",
          "    }",
          "    if (data.vehicle && data.vehicle._id) {",
          "        pm.collectionVariables.set('vehicleId', data.vehicle._id);",
          "    }",
          "}"
        ]
      }
    }
  ]
