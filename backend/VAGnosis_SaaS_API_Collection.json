        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@example.com\",\n  \"password\": \"Password123\",\n  \"profile\": {\n    \"name\": \"Test User\",\n    \"phone\": \"+254700000000\",\n    \"country\": \"Kenya\"\n  }\n}"
            },
